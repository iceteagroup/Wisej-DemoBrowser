        "pdf.js": "https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.6.347/pdf.min.js"
      }
    },
    "local": {
      "root": "Kendo",
      "packages": {
        "jquery.js": "%root%/js/jquery.min.js",
        "kendo.all.js": "%root%/js/kendo.all.min.js",
