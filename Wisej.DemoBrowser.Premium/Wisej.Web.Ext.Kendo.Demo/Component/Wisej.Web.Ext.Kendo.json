{
  //
  // Specifies which set of packages to use from the "sources" map below.
  //
  "source": "cdn",

  //
  // Specifies the default theme. Used when the Theme property of the widget is not set.
  //
  "theme": "bootstrap-main",

  //
  // Widget packages.
  //
  "sources": {
    "cdn": {
