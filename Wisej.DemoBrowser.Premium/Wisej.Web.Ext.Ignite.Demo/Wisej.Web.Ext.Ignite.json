{
  //
  // Specifies which set of packages to use from the "sources" map below.
  //
  "source": "cdn",

  //
  // Specifies the default theme. Used when the Theme property of the widget is not set.
  //
  "theme": "infragistics",

  //
  // Widget packages.
  //
  "sources": {
    "cdn": {
      "root": "http://cdn-na.infragistics.com/igniteui/2021.2/latest",
      "packages": {
        "infragistics.theme.css": "%root%/css/themes/%theme%/infragistics.theme.css",
        "infragistics.css": "%root%/css/structure/infragistics.css",
        "jquery.js": "https://code.jquery.com/jquery-3.5.1.min.js",
        "jquery-ui.js": "https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js",
        "infragistics.core.js": "%root%/js/infragistics.core.js",
        "infragistics.lob.js": "%root%/js/infragistics.lob.js",
        "infragistics.dv.js": "%root%/js/infragistics.dv.js",
        "infragistics-locale.js": "%root%/js/i18n/infragistics-%locale%.js"
      }
    },
    "local": {
      "root": "IgniteUI",
      "packages": {
        "infragistics.theme.css": "%root%/css/themes/infragistics/infragistics.theme.css",
        "infragistics.css": "%root%/css/structure/infragistics.css",
        "jquery.js": "%root/js/jquery.min.js",
        "jquery-ui.js": "%root/js/jquery-ui.min.js",
        "infragistics.core.js": "%root%/js/infragistics.core.js",
        "infragistics.lob.js": "%root%/js/infragistics.lob.js",
        "infragistics.dv.js": "%root%/js/infragistics.dv.js",
        "infragistics-locale.js": "%root%/js/i18n/infragistics-%locale%.js"
      }
    },
    "embedded": {
      "root": "resource.wx",
      "packages": {
        "infragistics.theme.css": "%root%/css/themes/infragistics/infragistics.theme.css",
        "infragistics.css": "%root%/css/structure/infragistics.css",
        "jquery.js": "%root/js/jquery.min.js",
        "jquery-ui.js": "%root/js/jquery-ui.min.js",
        "infragistics.core.js": "%root%/js/infragistics.core.js",
        "infragistics.lob.js": "%root%/js/infragistics.lob.js",
        "infragistics.dv.js": "%root%/js/infragistics.dv.js",
        "infragistics-locale.js": "%root%/js/i18n/infragistics-%locale%.js"
      }
    }
  }
}